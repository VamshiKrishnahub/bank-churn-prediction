    {
      "id": 8,
      "type": "table",
      "title": "Graph 8: Recent File Processing Details (Last 10 Files)",
      "description": "Detailed view of recent ingestions with quality metrics",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 32,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  file_name AS \"File Name\",\n  created_at AS \"Ingestion Time\",\n  total_rows AS \"Total Rows\",\n  valid_rows AS \"Valid Rows\",\n  invalid_rows AS \"Invalid Rows\",\n  ROUND((valid_rows::DECIMAL / NULLIF(total_rows, 0)) * 100, 2) AS \"Quality %\",\n  criticality AS \"Criticality\"\nFROM ingestion_statistics\nORDER BY created_at DESC\nLIMIT 10",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center"
          }
        },
        "overrides": [
