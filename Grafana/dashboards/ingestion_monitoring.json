              },
              {
                "color": "red",
                "value": 3
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 7,
      "type": "stat",
