{
  "id": null,
  "uid": "ingestion-monitoring-final",
  "title": "Dashboard 1: Ingested Data Monitoring",
  "tags": [],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Graph 1: Valid Data Percentage Over Time",
      "description": "Tracks data quality trend - ALERT when valid % drops below 60%",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  created_at AS time,\n  ROUND((valid_rows::DECIMAL / NULLIF(total_rows, 0)) * 100, 2) AS value\nFROM ingestion_statistics\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 3,
            "fillOpacity": 40,
            "showPoints": "auto"
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 65
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["mean", "min", "last"]
        }
      },
      "alert": {
        "uid": "valid_data_alert",
        "title": "Data Quality Alert - Valid % Below 60%",
        "condition": "A",
        "data": [
          {
            "refId": "A",
            "queryType": "",
            "relativeTimeRange": {
              "from": 600,
              "to": 0
            },
            "datasourceUid": "defence_db",
            "model": {
              "refId": "A",
              "rawSql": "SELECT \n  created_at AS time,\n  ROUND((valid_rows::DECIMAL / NULLIF(total_rows, 0)) * 100, 2) AS value\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '10 minutes'\nORDER BY time ASC",
              "format": "time_series"
            }
          },
          {
            "refId": "B",
            "queryType": "",
            "relativeTimeRange": {
              "from": 600,
              "to": 0
            },
            "datasourceUid": "__expr__",
            "model": {
              "type": "reduce",
              "refId": "B",
              "datasource": {
                "type": "__expr__",
                "uid": "__expr__"
              },
              "conditions": [
                {
                  "type": "query",
                  "evaluator": {
                    "params": [60],
                    "type": "lt"
                  },
                  "operator": {
                    "type": "and"
                  },
                  "query": {
                    "params": ["A"]
                  },
                  "reducer": {
                    "params": [],
                    "type": "avg"
                  }
                }
              ],
              "reducer": "mean",
              "expression": "A"
            }
          },
          {
            "refId": "C",
            "queryType": "",
            "relativeTimeRange": {
              "from": 600,
              "to": 0
            },
            "datasourceUid": "__expr__",
            "model": {
              "type": "threshold",
              "refId": "C",
              "datasource": {
                "type": "__expr__",
                "uid": "__expr__"
              },
              "conditions": [
                {
                  "type": "query",
                  "evaluator": {
                    "params": [60],
                    "type": "lt"
                  },
                  "operator": {
                    "type": "and"
                  },
                  "query": {
                    "params": ["B"]
                  }
                }
              ],
              "expression": "B",
              "thresholdConditions": [
                {
                  "evaluator": {
                    "params": [60],
                    "type": "lt"
                  }
                }
              ]
            }
          }
        ],
        "noDataState": "NoData",
        "execErrState": "Error",
        "for": "2m",
        "annotations": {
          "description": "Valid data percentage has dropped below 60%! Current value: {{ $values.B.Value }}%",
          "summary": "⚠️ ALERT: Data Quality Below Threshold"
        },
        "labels": {},
        "isPaused": false,
        "notification_settings": {
          "receiver": "Grafana_alert"
        }
      }
    },
    {
      "id": 2,
      "type": "bargauge",
      "title": "Graph 2: Data Quality Score - Current vs Past",
      "description": "Compares current quality with 1 hour ago and 24 hours ago",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  'Last 10 Minutes' AS metric,\n  ROUND((SUM(valid_rows)::DECIMAL / NULLIF(SUM(total_rows), 0)) * 100, 2) AS value\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '10 minutes'\nUNION ALL\nSELECT \n  '1 Hour Ago' AS metric,\n  ROUND((SUM(valid_rows)::DECIMAL / NULLIF(SUM(total_rows), 0)) * 100, 2) AS value\nFROM ingestion_statistics\nWHERE created_at BETWEEN NOW() - INTERVAL '1 hour 10 minutes' AND NOW() - INTERVAL '1 hour'\nUNION ALL\nSELECT \n  '24 Hours Ago' AS metric,\n  ROUND((SUM(valid_rows)::DECIMAL / NULLIF(SUM(total_rows), 0)) * 100, 2) AS value\nFROM ingestion_statistics\nWHERE created_at BETWEEN NOW() - INTERVAL '24 hours 10 minutes' AND NOW() - INTERVAL '24 hours'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 50
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "green",
                "value": 85
              }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "text": {
          "valueSize": 20
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Graph 3: Error Criticality Aggregation Over Time (Last 1 Hour)",
      "description": "Stacked area showing HIGH, MEDIUM, LOW criticality errors",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  SUM(CASE WHEN criticality = 'high' THEN invalid_rows ELSE 0 END) AS \"HIGH Criticality\",\n  SUM(CASE WHEN criticality = 'medium' THEN invalid_rows ELSE 0 END) AS \"MEDIUM Criticality\",\n  SUM(CASE WHEN criticality = 'low' THEN invalid_rows ELSE 0 END) AS \"LOW Criticality\"\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '1 hour'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 60,
            "showPoints": "never",
            "stacking": {
              "mode": "normal",
              "group": "A"
            }
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "HIGH Criticality"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.fillOpacity",
                "value": 70
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "MEDIUM Criticality"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.fillOpacity",
                "value": 60
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "LOW Criticality"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.fillOpacity",
                "value": 50
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["sum", "mean", "max"]
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Graph 4: Missing Value Errors Trend (Last 1 Hour)",
      "description": "Tracks if missing value errors are increasing or decreasing",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  SUM(CASE WHEN invalid_rows > 0 THEN invalid_rows ELSE 0 END) AS \"Invalid Rows\"\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '1 hour'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 40,
            "showPoints": "always",
            "pointSize": 5
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "orange",
                "value": 10
              },
              {
                "color": "red",
                "value": 15
              }
            ]
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["mean", "max", "last"]
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Graph 5: Total Files Ingested (Last 10 Min)",
      "description": "Monitors pipeline health - total files processed",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  COUNT(*) AS value\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '10 minutes'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "green",
                "value": 3
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Graph 6: Files with 100% Invalid Data (Last 30 Min)",
      "description": "Critical schema issues - all rows invalid",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 8,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  COUNT(*) AS value\nFROM ingestion_statistics\nWHERE created_at > NOW() - INTERVAL '30 minutes'\n  AND invalid_rows = total_rows\n  AND total_rows > 0",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
