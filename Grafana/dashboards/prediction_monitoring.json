{
  "id": null,
  "uid": "prediction-monitoring",
  "title": "Dashboard 2: Data Drift & Prediction Monitoring",
  "tags": [],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-2h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h"]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Graph 1: Churn Rate - Current vs Training Baseline",
      "description": "Compares current churn predictions against training baseline (20%) - detects prediction drift",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  ROUND((COUNT(*) FILTER (WHERE prediction = 1)::DECIMAL / NULLIF(COUNT(*), 0)) * 100, 1) AS value\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 3,
            "fillOpacity": 40,
            "showPoints": "auto"
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 10
              },
              {
                "color": "green",
                "value": 15
              },
              {
                "color": "green",
                "value": 25
              },
              {
                "color": "orange",
                "value": 35
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "last", "min", "max"]
        }
      }
    },
    {
      "id": 2,
      "type": "barchart",
      "title": "Graph 2: Age Distribution - Training Baseline vs Current Serving",
      "description": "Detects age drift between training and serving data - Green bars=baseline, Orange bars=current",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "WITH training_baseline AS (\n  SELECT '18-24' AS age_group, 15.0 AS percentage, 1 as sort_order UNION ALL\n  SELECT '25-34', 25.0, 2 UNION ALL\n  SELECT '35-44', 30.0, 3 UNION ALL\n  SELECT '45-54', 20.0, 4 UNION ALL\n  SELECT '55-64', 8.0, 5 UNION ALL\n  SELECT '65+', 2.0, 6\n),\nserving_current AS (\n  SELECT \n    CASE \n      WHEN age < 25 THEN '18-24'\n      WHEN age < 35 THEN '25-34'\n      WHEN age < 45 THEN '35-44'\n      WHEN age < 55 THEN '45-54'\n      WHEN age < 65 THEN '55-64'\n      ELSE '65+'\n    END AS age_group,\n    ROUND((COUNT(*)::DECIMAL / NULLIF((SELECT COUNT(*) FROM predictions WHERE created_at > NOW() - INTERVAL '30 minutes'), 0)) * 100, 1) AS percentage\n  FROM predictions\n  WHERE created_at > NOW() - INTERVAL '30 minutes'\n  GROUP BY age_group\n)\nSELECT \n  t.age_group AS metric,\n  t.percentage AS \"Training Baseline %\",\n  COALESCE(s.percentage, 0) AS \"Current Serving %\"\nFROM training_baseline t\nLEFT JOIN serving_current s ON t.age_group = s.age_group\nORDER BY t.sort_order",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Training Baseline %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Current Serving %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "orientation": "auto",
        "xTickLabelRotation": 0,
        "stacking": "none",
        "showValue": "always",
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Graph 3: Prediction Class Distribution - Aggregated Over Time",
      "description": "Stacked histogram showing Churn (1) vs No-Churn (0) predictions per minute",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  SUM(CASE WHEN prediction = 1 THEN 1 ELSE 0 END) AS \"Will Churn (1)\",\n  SUM(CASE WHEN prediction = 0 THEN 1 ELSE 0 END) AS \"Will Not Churn (0)\"\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 80,
            "showPoints": "never",
            "stacking": {
              "mode": "normal",
              "group": "A"
            }
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Will Churn (1)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Will Not Churn (0)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["sum", "mean", "max"]
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Graph 4: Average Balance Drift - Training vs Serving",
      "description": "Monitors if customer balance is drifting from training baseline ($75k)",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  ROUND(AVG(balance)::NUMERIC, 0) AS \"Current Avg Balance\"\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        },
        {
          "refId": "B",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  75000 AS \"Training Baseline ($75k)\"\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "auto"
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 50000
              },
              {
                "color": "yellow",
                "value": 65000
              },
              {
                "color": "green",
                "value": 70000
              },
              {
                "color": "green",
                "value": 85000
              },
              {
                "color": "orange",
                "value": 100000
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Training Baseline ($75k)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              },
              {
                "id": "custom.lineStyle",
                "value": {
                  "dash": [10, 5],
                  "fill": "dash"
                }
              },
              {
                "id": "custom.lineWidth",
                "value": 3
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["mean", "last"]
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Graph 5: Geography Distribution Drift Over Time",
      "description": "Detects categorical drift - shows percentage for each geography (33% baseline each)",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  ROUND((COUNT(*) FILTER (WHERE geography = 'France')::DECIMAL / NULLIF(COUNT(*), 0)) * 100, 1) AS \"France %\",\n  ROUND((COUNT(*) FILTER (WHERE geography = 'Spain')::DECIMAL / NULLIF(COUNT(*), 0)) * 100, 1) AS \"Spain %\",\n  ROUND((COUNT(*) FILTER (WHERE geography = 'Germany')::DECIMAL / NULLIF(COUNT(*), 0)) * 100, 1) AS \"Germany %\"\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nHAVING COUNT(*) > 0\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 40,
            "showPoints": "auto",
            "stacking": {
              "mode": "none"
            }
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "France %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Spain %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Germany %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["mean", "last"]
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Graph 6: Model Stuck Detection - Zero Predictions",
      "description": "Alerts if model predicts 0% or 100% churn (model is stuck)",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  ROUND((COUNT(*) FILTER (WHERE prediction = 1)::DECIMAL / NULLIF(COUNT(*), 0)) * 100, 1) AS value\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '10 minutes'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 5
              },
              {
                "color": "green",
                "value": 15
              },
              {
                "color": "green",
                "value": 25
              },
              {
                "color": "orange",
                "value": 35
              },
              {
                "color": "red",
                "value": 95
              }
