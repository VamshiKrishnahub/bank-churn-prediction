    {
      "id": 3,
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "barchart",
      "title": "ðŸ“ˆ Age Distribution Drift (Last 2h vs Last 10min)",
      "description": "INSIGHT: Compare age distribution. Significant differences indicate DATA DRIFT. Example: Training had 35-44 peak, serving has 55-64 peak = distribution shift.",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "WITH baseline_data AS (\n  SELECT \n    'Baseline (Last 2h)' AS dataset,\n    CASE \n      WHEN age < 25 THEN '18-24'\n      WHEN age < 35 THEN '25-34'\n      WHEN age < 45 THEN '35-44'\n      WHEN age < 55 THEN '45-54'\n      WHEN age < 65 THEN '55-64'\n      ELSE '65+'\n    END AS age_group,\n    COUNT(*) AS count\n  FROM predictions\n  WHERE created_at BETWEEN NOW() - INTERVAL '2 hours' AND NOW() - INTERVAL '1 hour'\n  GROUP BY dataset, age_group\n),\ncurrent_data AS (\n  SELECT \n    'Current (Last 10min)' AS dataset,\n    CASE \n      WHEN age < 25 THEN '18-24'\n      WHEN age < 35 THEN '25-34'\n      WHEN age < 45 THEN '35-44'\n      WHEN age < 55 THEN '45-54'\n      WHEN age < 65 THEN '55-64'\n      ELSE '65+'\n    END AS age_group,\n    COUNT(*) AS count\n  FROM predictions\n  WHERE created_at > NOW() - INTERVAL '10 minutes'\n  GROUP BY dataset, age_group\n)\nSELECT \n  age_group AS metric,\n  SUM(CASE WHEN dataset = 'Baseline (Last 2h)' THEN count ELSE 0 END) AS \"Baseline\",\n  SUM(CASE WHEN dataset = 'Current (Last 10min)' THEN count ELSE 0 END) AS \"Current\"\nFROM (\n  SELECT * FROM baseline_data\n  UNION ALL\n  SELECT * FROM current_data\n) combined\nGROUP BY age_group\nORDER BY age_group",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Baseline"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Current"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "orientation": "auto",
        "xTickLabelRotation": 0,
        "stacking": "none",
        "showValue": "auto",
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "legend": {
          "showLegend": true,
          "displayMode": "list",
          "placement": "bottom"
        }
      }
