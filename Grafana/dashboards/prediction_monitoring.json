      "type": "timeseries",
      "title": "ðŸ“Š Prediction Volume by Source (Last 2 Hours)",
      "description": "INSIGHT: Detect pipeline failures. If scheduled predictions drop to 0, prediction DAG is broken. If webapp drops, user engagement issues.",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  DATE_TRUNC('minute', created_at) AS time,\n  COUNT(*) FILTER (WHERE source = 'webapp') AS \"Webapp Predictions\",\n  COUNT(*) FILTER (WHERE source = 'scheduled') AS \"Scheduled Predictions\"\nFROM predictions\nWHERE created_at > NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time ASC",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
