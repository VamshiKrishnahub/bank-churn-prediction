    {
      "id": 8,
      "type": "table",
      "title": "Graph 8: Recent Predictions Summary (Last 20 Predictions)",
      "description": "Detailed view - Green=No Churn, Red=Will Churn, Age colors show drift",
      "datasource": {
        "type": "postgres",
        "uid": "defence_db"
      },
      "gridPos": {
        "x": 16,
        "y": 24,
        "w": 8,
        "h": 8
      },
      "targets": [
        {
          "refId": "A",
          "rawSql": "SELECT \n  CASE WHEN prediction = 1 THEN 'Will Churn' ELSE 'No Churn' END AS \"Prediction\",\n  age AS \"Age\",\n  ROUND(balance::NUMERIC, 0) AS \"Balance\",\n  geography AS \"Geography\",\n  gender AS \"Gender\",\n  created_at AS \"Time\"\nFROM predictions\nORDER BY created_at DESC\nLIMIT 20",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center"
          }
        },
